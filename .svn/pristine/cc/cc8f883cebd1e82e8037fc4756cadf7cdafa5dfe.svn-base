textarea {
width:700rpx;
height:300rpx;
display:block;
position:relative;
border:1px solid #e6e6e6;
}
input {
display:block;
height:2rem;
text-overflow:clip;
overflow:hidden;
white-space:nowrap;
font-family:UICTFontTextStyleBody;
min-height:1.4rem;
border-bottom: 1px solid #e6e6e6;
}
.pic image{
  width: 100rpx;
  height:100rpx;
}
button[type="primary"] {
color:#FFFFFF;
background-color:#17b990;

}
radio .wx-radio-input {
-webkit-appearance:none;

margin-right:5px;
outline:0;
border:1px solid #D1D1D1;
border-radius:50%;
width:40rpx;
height:40rpx;
position:relative;

}

