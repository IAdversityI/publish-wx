/**index.wxss**/

.title {
  /* height: 300rpx; */
  width: 100%;
  overflow: hidden;
}

.box {
  overflow: hidden;
  border: 1px solid #e6e6e6;
  margin: 0 auto;
  width: 94%;
  margin-top: 30rpx;
}

.content {
  /* width: 600rpx; */
  width: 100%;
  height: 62rpx;
  overflow: hidden;
  background-color: #ededed;
  line-height: 62rpx;
  border-top: 1px solid #e6e6e6;
}

.contentInput {
  width: 98%;
  height: 85rpx;
  overflow: hidden;
  /* margin-left: 20rpx; */
}

input {
  display: block;
  height: 55rpx;
  text-overflow: clip;
  overflow: hidden;
  white-space: nowrap;
  font-family: UICTFontTextStyleBody;
  min-height: 1.4rem;
  border: 1px solid #e6e6e6;
  font-size: 32rpx;
  padding-left: 10rpx;
  margin-top: 10rpx;
  margin-left: 16rpx;
  color: #999;
}

.areaBox {
  overflow: hidden;
  /* padding-left: 20rpx; *//* margin-top: 30rpx; */
  font-size: 32rpx;
}

.title image {
  width: 200rpx;
  height: 200rpx;
  display: inline-block;
  overflow: hidden;
  padding-left: 36%;
  padding-top: 10rpx;
  margin: 0 auto;
}

.titleName {
  /* font-size: 20rpx; */
  padding-left: 80%;
}

text {
  font-size: 32rpx;
  /* height: 32rpx; *//* line-height: 32rpx; */
  color: #999;
}

image {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  /* overflow: hidden; *//* padding-left: 40%; */
  padding-top: 4rpx;
}

textarea {
  width: 100%;
  height: 300rpx;
  display: block;
  position: relative;
  /* border: 1px solid #e6e6e6; */
  font-size: 32rpx;
  margin: 10rpx;
  color: #999;
}

.pic {
  margin-left: 18rpx;
}

.pic image {
  width: 100rpx;
  height: 100rpx;
}

.board-item button[type="primary"] {
  color: #fff;
  background-color: #17b990;
  width: 80%;
}

.addPic {
  float: left;
  margin-right: 14rpx;
  margin-top: 10rpx;
}

.img-item {
  float: left;
  margin-right: 14rpx;
  padding-top: 10rpx;
}

radio-group {
  display: block;
  margin-top: 20rpx;
  margin-left: 20rpx;
}

.radio {
  margin-right: 12rpx;
}

.color_font {
  color: #17b990;
}

button[type="primary"] {
  color: #fff;
  background-color: #17b990;
  height: 80rpx;
  line-height: 80rpx;
}

.arriveTextArea{
width:100%;
height:200rpx;
display:block;
position:relative;
font-size:32rpx;
color:#999;
margin:10rpx auto;
/* margin-left:10rpx; */



}

